DOXYFILE_ENCODING = UTF-8
PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
PROJECT_BRIEF = "A cross-platform DSP library written in C++."
PROJECT_LOGO = @PROJECT_SOURCE_DIR@/logo.svg
OUTPUT_LANGUAGE = English


STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@/include/edsp \
                         @PROJECT_BINARY_DIR@/include/edsp

INPUT                  = @PROJECT_SOURCE_DIR@/include/edsp \
                         @PROJECT_BINARY_DIR@/include/edsp \
                         @PROJECT_SOURCE_DIR@/README.md
USE_MDFILE_AS_MAINPAGE = @PROJECT_SOURCE_DIR@/README.md
FILE_PATTERNS          = *.hpp \
                         *.h
GENERATE_XML = NO
GENERATE_LATEX = NO
GENERATE_HTML = YES
GENERATE_DOCBOOOK = YES
RECURSIVE = YES
EXTRACT_ALL = YES
EXTRACT_PRIVATE = NO
SHOW_FILES = YES
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
RECURSIVE              = YES
SKIP_FUNCTION_MACROS = YES
HIDE_UNDOC_MEMBERS     = YES
HIDE_UNDOC_CLASSES     = YES
USE_MATHJAX = YES
INHERIT_DOCS = YES
CREATE_SUBDIRS = YES
CALL_GRAPH = NO
CALLER_GRAPH = NO
HAVE_DOT = NO
EXCLUDE_SYMBOLS = internal \
                DECLARE_* \
                *_v
EXCLUDE_PATTERNS = */meta/* \
                   */internal/* \
                   */test/* \
                   */benchmark/* \



